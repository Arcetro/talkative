{
  "name": "workflow-agent-backend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
